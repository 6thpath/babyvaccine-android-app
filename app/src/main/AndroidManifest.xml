<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.t6.babyvaccin.babyvaccin">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Register" />
        <activity android:name=".BabyAdd" />
        <activity android:name=".BabyDetail" />
        <activity android:name=".HomeNav" />
        <activity android:name=".VaccineAdd" />
        <activity android:name=".VaccineDetail" />
        <activity android:name=".InjectLocationAdd" />
        <activity android:name=".InjectLocationDetail" />
    </application>

</manifest>